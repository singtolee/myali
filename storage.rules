service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read: if request.auth!=null;
    }

    match /BANK-IMG/{image} {
      allow write: if request.auth.uid == "nRVepbMHCNcnvkG0yKRIlcVKnkU2";
    }

    match /BANNER/{image} {
      allow write: if request.auth.uid == "nRVepbMHCNcnvkG0yKRIlcVKnkU2";
    }

    match /CATEGORY-IMG/{image} {
      allow write: if request.auth.uid == "nRVepbMHCNcnvkG0yKRIlcVKnkU2";
    }

    match /BANKTRANSFERBILLS/{image} {
      allow write: if request.auth!=null;
    }


  }
}
